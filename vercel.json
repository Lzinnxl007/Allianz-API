{
    "version": 2,
    "builds": [
      {
        "src": "src/server/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/src/server/server.js",
        "dest": "/src/server/$1"
      },
      {
        "src": "/spot",
        "dest": "/spot"
      }
    ]
  }
  